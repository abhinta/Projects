<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.loginViewController">
   <children>
      <ImageView fitHeight="123.0" fitWidth="123.0" layoutX="50.0" layoutY="82.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@cdss-store-login-icon.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtId" layoutX="271.0" layoutY="91.0" promptText="user id" />
      <PasswordField fx:id="txtPassword" layoutX="271.0" layoutY="138.0" promptText="password" />
      <Button fx:id="btnLogin" layoutX="311.0" layoutY="189.0" mnemonicParsing="false" onAction="#doLoginIn" prefHeight="25.0" prefWidth="57.0" text="Log In">
         <font>
            <Font name="Georgia" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
